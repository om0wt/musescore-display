{
  "name": "musescore-display",
  "version": "0.1.0",
  "description": "Display MuseScore .mscz/.mscx files in the browser via OpenSheetMusicDisplay",
  "main": "build/musescore-display.min.js",
  "types": "build/dist/src/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --config webpack.dev.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:demo": "webpack --config webpack.ghpages.js",
    "build:site": "npm run build:demo && typedoc --out dist-demo/docs",
    "docs": "typedoc"
  },
  "dependencies": {
    "jspdf": "^4.2.0",
    "jszip": "^3.10.1",
    "opensheetmusicdisplay": "^1.9.6"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.28.17",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "webpack-merge": "^5.10.0"
  },
  "author": "Pavol Calfa & Claude (Anthropic)",
  "license": "MIT"
}
